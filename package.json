{
  "name": "smart-form-filler",
  "version": "2.0.0",
  "description": "Smart Form Filler - AI-powered browser extension for intelligent data extraction and form filling",
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:demo": "cd demo && npm install",
    "install:all": "npm run install:backend && npm run install:demo",
    "dev:backend": "cd backend && npm run dev",
    "dev:demo": "cd demo && npm start",
